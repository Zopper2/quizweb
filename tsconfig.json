{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/categories/[category]/page.jsx", "src/app/page.tsx", "src/app/signup/page.tsx", "src/app/login/page.jsx", "src/app/quiz/[id]/page.tsx", "src/app/components/quizcard/page.tsx", "src/app/admin/add-quiz/page.jsx", "src/app/api/auth/register/route.js", "src/app/api/quiz/category/details", "src/app/api/quiz/category/[category]/route.js", "src/app/user/page.jsx", "src/app/user/page.jsx", "src/app/quiz/page.jsx"],
  "exclude": ["node_modules"]
}
